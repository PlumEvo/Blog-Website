<h1><%= data.title %></h1>

<img src= "https://source.unsplash.com/random/900x600?sad"  alt="card 1 image" class="card_image" loading="lazy">

<article class="article">
    <%= data.body %>
    <div class="lidi">
        <i class="up set"></i>
        <i class="down"></i>
    </div>
</article>

<script>
    const like = document.querySelector(".up");
    const dislike = document.querySelector(".down");
    like.addEventListener('click',function(){
        this.classList.toggle("set");
        dislike.classList.toggle("set");
    });
    dislike.addEventListener("click",function(){
        this.classList.toggle("set");
        like.classList.toggle("set");
    });
</script>